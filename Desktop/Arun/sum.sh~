
echo "Enter 1st Number:"
read a
echo "Enter 2nd Number:"
read b
sum=`expr $a + $b`
echo "Sum of 2 Numbers:$sum"
div=`expr $a - $b`
echo "Difference of 2 Numbers:$div"
mul=`expr $a \* $b`
echo "Multiplication of 2 Numbers:$mul"
div=`expr $a / $b`
echo "Quotient of 2 Numbers:$div"
mod=`expr $a % $b`
echo "Mode of 2 Numbers:$mod"
